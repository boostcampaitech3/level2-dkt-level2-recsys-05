{
    "name": "모델이름",
    "entity" : "wandb project 만든 사람 계정명",
    
    "cat_cols" : ["사용할 cat cols"], 
    "num_cols" : ["사용할 num cols"],


    "preprocess": {
        "type": "TransformerPreprocess",
        "args": {
            "data_dir": "/opt/ml/input/data"
        }
    },

    "dataset" : {
        "type": "TransformerDataset",
        "args": {
            "data_augmentation" : false
        }
    },

    "data_loader" : {
        "type": "TransformerDataLoader",
        "args": {
            "batch_size" : 32,
            "shuffle" : true,
            "num_workers" : 8,
            "collate_fn" : "transformer_collate"
        }
    },

    "arch" : {
        "type": "TransformerAndLSTMEncoderDeocoderEachEmbedding",
        "args": {
            "hidden_dim" : 128,
            "embedding_size" : 64,
            "num_heads" : 2,
            "num_layers" : 1,
            "dropout_rate" : 0.5
        }
    },

    "loss" : "bce_loss",

    "optimizer" : {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },

    "trainer" : {
        "type" : "TransformerTrainer",
        "epochs": 1,
        "oof" : 5,
        "seed" : 22,
        "save_dir": "/opt/ml/model",
        "submission_dir": "/opt/ml/submission" 
    }
}
